{
    "name": "helsingborg-stad/modularity-testimonials",
    "type": "wordpress-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Nikolas Ramstedt",
            "email": "nikolas.ramstedt@helsingborg.se"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "installer-paths": {
            "vendor/{$vendor}/{$name}": [
                "type:wordpress-muplugin",
                "type:wordpress-theme",
                "type:wordpress-plugin",
                "type:acf-plugin"
            ]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/helsingborg-stad/modularity-testimonials.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "ModularityTestimonials\\": "source/php/"
        }
    },
    "require": {
        "helsingborg-stad/municipio": "^6.0.0"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    }
}